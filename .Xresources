URxvt.font:         xft:Iosevka Term:style=Regular:size=14
URxvt.scrollBar:    false
Xft.antialias:      true
Xft.rgba:           rgb
Xft.hinting:        true
Xft.autohint:       false
Xft.hintstyle:      hintmedium
Xft.lcdfilter:      lcddefault
URxvt*dynamicColors: on

!Rofi
rofi.lines:         5
rofi.width:         30
rofi.bw:            5

rofi.color-window:  #1a1917,#c34564,#d0d0d0
rofi.color-normal:  #1a1917,#d0d0d0,#302e2d,#c34564,#302e2d
rofi.color-active:  #85a300,#302e2d,#85a300,#c34564,#302e2d
rofi.color-urgent:  #446fbd,#302e2d,#446fbd,#c34564,#302e2d

rofi.hide-scrollbar: true
rofi.font:        Iosevka Regular 11
rofi.separator-style: solid
