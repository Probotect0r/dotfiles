URxvt.font:         xft:Iosevka:style=Regular:size=12
URxvt.scrollBar:    false
!URxvt.lineSpace:	  3
Xft.antialias:      true
Xft.rgba:           rgb
Xft.hinting:        true
Xft.autohint:       false
Xft.hintstyle:      hintmedium
Xft.lcdfilter:      lcddefault
URxvt*dynamicColors: on

!Rofi
rofi.lines:         5
rofi.width:         30
rofi.bw:            5

rofi.color-window:  #201f1d,#c34564,#cfcfcf
rofi.color-normal:  #201f1d,#cfcfcf,#33312E,#c34564,#33312E
rofi.color-active:  #85a300,#33312E,#85a300,#c34564,#33312E
rofi.color-urgent:  #6c9ef8,#33312E,#6c9ef8,#c34564,#33312E

rofi.hide-scrollbar: true
rofi.font:        Iosevka Regular 11
rofi.separator-style: solid
