URxvt.font:         xft:Iosevka:style=Regular:size=12
URxvt.scrollBar:    false
!URxvt.lineSpace:	  3
Xft.antialias:      true
Xft.rgba:           rgb
Xft.hinting:        true
Xft.autohint:       false
Xft.hintstyle:      hintmedium
Xft.lcdfilter:      lcddefault
URxvt*dynamicColors: on

!Rofi
rofi.lines:         5
rofi.width:         30
rofi.bw:            5

rofi.color-window:  #2c2833,#bf6257,#dbd7b8
