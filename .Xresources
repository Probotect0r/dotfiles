URxvt.font:         xft:Iosevka:style=Regular:size=12
URxvt.scrollBar:    false
!URxvt.lineSpace:	  3
Xft.antialias:      true
Xft.rgba:           rgb
Xft.hinting:        true
Xft.autohint:       false
Xft.hintstyle:      hintmedium
Xft.lcdfilter:      lcddefault
URxvt*dynamicColors: on

!Rofi
rofi.lines:         5
rofi.width:         30
rofi.bw:            5

rofi.color-window:  #2c292b,#ab6d4e,#c3b495
rofi.color-normal:  #2c292b,#c3b495,#312e30,#ab6d4e,#312e30
rofi.color-active:  #88885d,#312e30,#88885d,#ab6d4e,#312e30
rofi.color-urgent:  #45626e,#312e30,#45626e,#ab6d4e,#312e30

rofi.hide-scrollbar: true
rofi.font:        Iosevka Regular 11
rofi.separator-style: solid
