rofi.color-active:  #82a37c,#322d39,#82a37c,#bf6257,#322d39
rofi.color-urgent:  #6d6a80,#322d39,#6d6a80,#bf6257,#322d39

rofi.hide-scrollbar: true
rofi.font:        Iosevka Regular 11
rofi.separator-style: solid
