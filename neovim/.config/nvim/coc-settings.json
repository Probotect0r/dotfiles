{
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": " ",
  "diagnostic.hintSign": "",
  "yaml.schemastore.enable": "true",
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    },
    "elixirLS": {
      "command": "/opt/elixir-ls/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    }
  }
}
